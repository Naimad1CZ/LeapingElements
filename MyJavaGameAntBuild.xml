            <classpath refid="myjavagame.module.classpath"/>
            <src refid="myjavagame.module.test.sourcepath"/>
            <patternset refid="excluded.from.compilation.myjavagame"/>
        </javac2>

        <copy todir="${myjavagame.testoutput.dir}">
            <fileset dir="${module.myjavagame.basedir}/src/test/java">
                <patternset refid="compiler.resources"/>
                <type type="file"/>
            </fileset>
        </copy>
    </target>

    <target name="clean.module.myjavagame" description="cleanup module">
