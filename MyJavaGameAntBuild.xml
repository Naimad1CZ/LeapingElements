    <target name="compile.module.myjavagame"
            depends="compile.module.myjavagame.production,compile.module.myjavagame.tests"
            description="Compile module MyJavaGame"/>

    <target name="compile.module.myjavagame.production" depends="register.custom.compilers"
            description="Compile module MyJavaGame; production classes">
        <mkdir dir="${myjavagame.output.dir}"/>
        <javac2 destdir="${myjavagame.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}"
                memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.myjavagame}/javac">
            <compilerarg line="${compiler.args.myjavagame}"/>
            <bootclasspath refid="myjavagame.module.bootclasspath"/>
            <classpath refid="myjavagame.module.production.classpath"/>
            <src refid="myjavagame.module.sourcepath"/>
            <patternset refid="excluded.from.compilation.myjavagame"/>
        </javac2>

        <copy todir="${myjavagame.output.dir}">
            <fileset dir="${module.myjavagame.basedir}/src/main/java">
                <patternset refid="compiler.resources"/>
                <type type="file"/>
            </fileset>
            <fileset dir="${module.myjavagame.basedir}/src/main/resources">
                <patternset refid="compiler.resources"/>
                <type type="file"/>
            </fileset>
        </copy>
    </target>

    <target name="compile.module.myjavagame.tests"
            depends="register.custom.compilers,compile.module.myjavagame.production"
            description="compile module MyJavaGame; test classes" unless="skip.tests">
        <mkdir dir="${myjavagame.testoutput.dir}"/>
        <javac2 destdir="${myjavagame.testoutput.dir}" debug="${compiler.debug}"
                nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true"
